<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/seatme_background"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="-246dp">


        <Spinner
            android:id="@+id/branch_sppiner"
            android:layout_width="330dp"
            android:layout_height="40dp"
            android:background="@color/white"
            android:textColor="@color/black"
            android:visibility="visible"
            app:backgroundTint="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.489"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.198"
            tools:visibility="visible" />

        <Button
            android:id="@+id/change"
            android:layout_width="330dp"
            android:layout_height="40dp"
            android:layout_marginTop="23dp"
            android:text="הזמנת מקום"
            android:textColor="@color/black"
            app:backgroundTint="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/branch_sppiner"
            app:layout_constraintStart_toStartOf="@+id/branch_sppiner"
            app:layout_constraintTop_toBottomOf="@+id/branch_sppiner"
            app:layout_constraintVertical_bias="0.023" />



    <Button
        android:id="@+id/review"
        android:layout_width="330dp"
        android:layout_height="40dp"
        android:text="ההזמנות שלי"
        android:textColor="@color/black"
        app:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/diary"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/diary"
        app:layout_constraintTop_toBottomOf="@+id/diary"
        app:layout_constraintVertical_bias="0.146" />



        <Button
        android:id="@+id/diary"
        android:layout_width="330dp"
        android:layout_height="40dp"
        android:layout_marginTop="32dp"
        android:text="דירוג המסעדה"
        android:textColor="@color/black"
        app:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/menu"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="@+id/menu"
        app:layout_constraintTop_toBottomOf="@+id/menu"
        app:layout_constraintVertical_bias="0.035" />

    <Button
        android:id="@+id/menu"
        android:layout_width="330dp"
        android:layout_height="40dp"
        android:layout_marginTop="27dp"
        android:text="צפייה בתפריט"
        android:textColor="@color/black"
        app:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/change"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/change"
        app:layout_constraintTop_toBottomOf="@+id/change"
        app:layout_constraintVertical_bias="0.023" />

        <Button

            android:id="@+id/logout"
            android:layout_width="330dp"
            android:layout_height="40dp"
            android:text="התנתק"
            android:textColor="@color/black"
            app:backgroundTint="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/diary"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/diary"
            app:layout_constraintTop_toBottomOf="@+id/diary"
            app:layout_constraintVertical_bias="0.441" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="137dp"
        android:layout_height="104dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/seatme_logo" />


</androidx.constraintlayout.widget.ConstraintLayout>